<%- @page_title = "Edit #{@team.name}" -%>
<h1>Edit <%= h(@team.name) || 'Team' %></h1>

<%= error_messages_for :team %>

<% form_for(:team, :url => team_path(@team), :html => { :method => :put }) do |f| %>
  <%= render :partial=>'form', :object=>f %>
  <p><%= submit_tag "Update" %></p>
<% end %>

<p><%= link_to 'Back', team_path(@team) %></p>